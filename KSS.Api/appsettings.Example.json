{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Type": "JwtBearer"
  },
  "TokenSetting": {
    "JwtSecret": "<YOUR-JWT-SECRET-MINIMUM-32-CHARACTERS>"
  },
  "Port": 8000,
  "AllowedOrigins": [
    "http://localhost:3000",
    "http://localhost:5055",
    "http://localhost:8000",
    "https://localhost:7043",
    "https://localhost:8001"
  ],
  "Certificate": {
    "pfxFilePath": "",
    "pfxPassword": ""
  },
  "FileSetting": {
    "TempDirectory": {
      "Excel": "C:\\Temp\\Excel\\",
      "Notepad": "C:\\Temp\\Notepad\\",
      "DBF": "C:\\Temp\\DBF\\",
      "RAR": "C:\\Temp\\RAR\\"
    },
    "Extension": {
      "Excel": "xlsx",
      "Notepad": "txt",
      "DBF": "dbf",
      "RAR": "rar"
    },
    "InsuranceFileName": {
      "ExcelWOR": "ExcelWOR",
      "ExcelKAR": "ExcelKAR",
      "NotepadWOR": "NotepadWOR",
      "NotepadKAR": "NotepadKAR",
      "DBFWOR": "DBFWOR",
      "DBFKAR": "DBFKAR",
      "ExcelERR": "ExcelERR",
      "RAR": "RAR"
    }
  },
  "ConnectionStrings": {
    "KSSMain": "Data Source=<SERVER>;Initial Catalog=<DATABASE>;User ID=<USER>;Password=<PASSWORD>;TrustServerCertificate=True;",
    "KSSLog": "Data Source=<SERVER>;Initial Catalog=<LOG_DATABASE>;User ID=<USER>;Password=<PASSWORD>;TrustServerCertificate=True;"
  }
}
